<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Fortrade Pages">
    <test name="Demo Account Registration - FSC Regulation">
        <classes>
            <class name="TestsFortrade.EducationLp"/>
            <parameter name="countryCode" value="381"/>
            <parameter name="regulation" value="FSC"/>
        </classes>
    </test>
    <test name="Demo Account Registration - FCA Regulation">
        <classes>
            <class name="TestsFortrade.EducationLp"/>
            <parameter name="countryCode" value="44"/>
            <parameter name="regulation" value="FCA"/>
        </classes>
    </test>
    <test name="Demo Account Registration - Asic Regulation">
        <classes>
            <class name="TestsFortrade.EducationLp"/>
            <parameter name="countryCode" value="61"/>
            <parameter name="regulation" value="Asic"/>
        </classes>
    </test>
    <test name="Demo Account Registration - Iiroc Regulation">
        <classes>
            <class name="TestsFortrade.EducationLp"/>
            <parameter name="countryCode" value="1"/>
            <parameter name="regulation" value="iiroc"/>
        </classes>
    </test>
