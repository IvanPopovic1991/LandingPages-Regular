<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Fortrade Pages">
    <listeners>
        <listener class-name="listeners.ScreenshotListener"/>
        <listener class-name="listeners.Transform"/>
    </listeners>
    <test name="Demo Account Registration - FSC Regulation">
        <parameter name="regulation" value="FSC"/>
        <classes>
            <class name="TestsFortrade.EducationLp">
                <methods>
                    <include name="emptyDemoAccountRegistration"/>
                    <include name="unsuccessfullyDemoAccountRegistration"/>
                </methods>
            </class>
        </classes>
    </test>
<test name="Demo Account Registration - FCA Regulation">
        <parameter name="regulation" value="FCA"/>
        <classes>
            <class name="TestsFortrade.EducationLp">
                <methods>
                    <include name="emptyDemoAccountRegistration"/>
                    <include name="unsuccessfullyDemoAccountRegistration"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Demo Account Registration - Asic Regulation">
        <parameter name="regulation" value="Asic"/>
        <classes>
            <class name="TestsFortrade.EducationLp">
                <methods>
                    <include name="emptyDemoAccountRegistration"/>
                    <include name="unsuccessfullyDemoAccountRegistration"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Demo Account Registration - Iiroc Regulation">
        <parameter name="regulation" value="iiroc"/>
        <classes>
            <class name="TestsFortrade.EducationLp">
                <methods>
                    <include name="emptyDemoAccountRegistration"/>
                    <include name="unsuccessfullyDemoAccountRegistration"/>
                </methods>
            </class>
        </classes>
    </test>
